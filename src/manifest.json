{
  "name": "Urltransfer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Transfer url(or small text) to you smart phone.",
  "icons": { "16": "icon16.png",
			 "48": "icon48.png",
             "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://chart.googleapis.com/",
	 "tabs"
  ]
}

